<!-- Navigation -->
<nav>
  <i class="fa fa-bars remind"></i>
  <a class="title" href="<%= config.root %>"><%= config.title %></a>
  <ul class="normal">
    <li>
      <a href="<%= config.root %>">Home</a>
    </li>

    <% site.pages.forEach(function(page){ %>
      <% if(page.title){ %>
      <li>
        <a href="<%= config.root %><%= page.path.replace('index.html', '') %>"><%= page.title %></a>
      </li>
      <% } %>
    <% }); %>
  </ul>
</nav>
<%- js('js/responsive.js') %>
