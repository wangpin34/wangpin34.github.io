no-margin-padding()
  margin 0
  padding 0

normalize-ul-ol()
  no-margin-padding()
  li 
    list-style-type none

bg-img()
  content: ""
  position: absolute
  top: 0
  left: 0
  width: 100%
  height: 100%
  filter: brightness(0.7)
  background-repeat: no-repeat 
  background-position: center
  background-origin: border-box
  background-color: gray100
  background-size: cover
